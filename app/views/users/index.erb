<div id='wrapper'>
	<section>
		<h2>Registered Users</h2>
		<table>
			<tr>
				<td><h4>Name</h4></td><td><h4>Email</h4></td><td><h4>Twitter</h4></td>
			</tr>
			<% @users.each do |user| %>
				<tr>
					<td>
						<% if user.is_admin %>
							<span id='admin'><a href='users/<%= user.id %>'><%= user.name %></a></span>
						<% else %>
							<a href='users/<%= user.id %>'><%= user.name %></a>
						<% end %>
					</td>
					<td><%= user.email %></td>
					<td><%= user.twitter %></td>
				</tr>
			<% end %>
		</table>
	</section>
</div>